    <section class="breadcrumb_part">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb_iner">
                        <h3 style="color: antiquewhite;">Hello {{name}}</h3>
                        <h3 style="color: antiquewhite;">Your email: {{email}}</h3>
                        <a href="/login/sell" class="btn_3" style="background-color:white;transition: all 9.5s ease;">Add Product</a>
                        <a href="/login/logout" class="btn_3" style="background-color:white;transition: all 9.5s ease;">Logout</a>
                    </div>
                </div>
            </div>
        </div>
    </section>
