  <!--================Cart Area =================-->
  <section class="cart_area section_padding">
    <div class="container">
      <div class="cart_inner">
        <div class="table-responsive">
          <table class="table">
            <thead>
              <tr>
                <th scope="col">Product</th>
                <th scope="col">Price</th>
                <th scope="col">Quantity</th>
              </tr>
            </thead>
            <tbody>
              <h4>Your inventory: ({{totalproduct}}) items</h4>
              {{#each product_list}}
              <tr>
                <td>
                  <div class="media">
                    <div class="d-flex">
                      <img src="/img/{{this.product_picture}}" alt="" />
                    </div>
                    <div class="media-body">
                      <p>{{this.product_name}}</p>
                    </div>
                  </div>
                </td>
                <td>
                  <h5>{{this.product_price}}</h5>
                </td>
                <td>
                  <div class="product_count">
                    <span class="input-number-decrement"> <i class="ti-minus"></i></span>
                    <input class="input-number" type="text" value="{{this.product_quantity}}" min="0" max="9">
                    <span class="input-number-increment"> <i class="ti-plus"></i></span>
                  </div>
                </td>
                <td>
                  <form class="row contact_form" action="/login/clerkrequest/{{_id}}" method="post" style="display: inline-block;">
                          <button type="submit" value="submit" class="btn_3">
                          edit
                        </button>
                    </form>
                </td>
                <td>
                  <form class="row contact_form" action="/login/clerkdelete/{{_id}}" method="post" style="display: inline-block;">
                        <button type="submit" value="submit" class="btn_3" style="color: red;">
                          delete
                        </button>
                    </form>
                </td>

              </tr>
             {{/each}}
            
              <tr>
                <td></td>
                <td></td>
                <td></td>

                <td>
                  <h5>Subtotal</h5>
                </td>
                <td style="text-align: right;">
                  <h5>${{totalbill}}</h5>
                </td>
              </tr>
            </tbody>
          </table>
          
        </div>
      </div>
  </section>
  <!--================End Cart Area =================-->
